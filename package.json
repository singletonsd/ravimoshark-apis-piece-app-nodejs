{
  "name": "ravimoshark-piece-api",
  "version": "1.0.0",
  "description": "This is an API to interact with Ravimoshark piece database. This API allow to create, modify and delete pieces and products (cards, details and key points). This API does not contain any kind of user validation but It request to have a valid authentication user to interact with it. For more information about user Authentication, please refer to [Ravimoshark User Authentication](https://ravimoshark.com/back/apis/auth/latest).",
  "main": "src/index.js",
  "scripts": {
    "build-all": "npm run build",
    "build": "npm run tsc",
    "check": "npm-check",
    "start-ts": "ts-node src/server.ts -r dotenv/config",
    "start": "npm run tsc && NODE_ENV=production node dist/server.js",
    "start-pm2": "NODE_ENV=production pm2-runtime start dist/server.js",
    "lint": "tslint --project ./",
    "tsc": "rm -rf dist && tsc",
    "docker-download": "mkdir -p scripts && curl -o scripts/docker_build_multiple_images.sh -L https://singletonsd.gitlab.io/scripts/docker/latest/docker_build_multiple_images.sh && chmod +x scripts/docker_build_multiple_images.sh && ./scripts/docker_build_multiple_images.sh",
    "docker-generate": "./scripts/docker_build_multiple_images.sh",
    "generateEntities": "typeorm-model-generator -h d05qizmaog.database.windows.net -d db_taches_new -u XXXX -x XXXX -p 1433 -e mssql -o src/databases/entities --noConfig --generateConstructor --ssl",
    "generateTest": "oatts generate -s src/api/swagger.yaml -w test",
    "generate-oas": "oas-generator src/api/swagger.yaml -n generated",
    "generate-oas-local": "oas-generator ../documentation/dist/openapi.yaml -n generated",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "debug": "tsnd --inspect --respawn src/server.ts",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "test": "mocha -r ts-node/register --recursive test"
  },
  "keywords": [
    "swagger",
    "openapi",
    "ravimoshark",
    "singletonSD",
    "piece"
  ],
  "homepage": "https://gitlab.com/ravimosharksas/apis/piece/app-nodejs",
  "license": "SEE LICENSE IN LICENSE file",
  "bugs": {
    "url": "https://gitlab.com/ravimosharksas/apis/piece/app-nodejs/issues",
    "email": "patoperpetua@gmail.com"
  },
  "author": {
    "name": "Patricio Perpetua",
    "email": "patoperpetua@gmail.com",
    "url": "http://patricioperpetua.com"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/ravimosharksas/apis/piece/app-nodejs.git"
  },
  "private": true,
  "dependencies": {
    "@types/bcrypt": "3.0.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.2",
    "@types/express-jwt": "0.0.42",
    "@types/jsonwebtoken": "8.3.5",
    "@types/lodash": "4.14.149",
    "@types/node": "12.12.12",
    "@types/swagger-tools": "0.10.6",
    "body-parser": "^1.19.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "3.21.2",
    "js-yaml": "3.13.1",
    "keycloak-connect": "8.0.0",
    "mssql": "6.0.1",
    "normalize-port": "1.0.0",
    "oas-tools": "2.1.7",
    "reflect-metadata": "0.1.13",
    "request-promise": "4.2.5",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "0.2.20",
    "typescript": "3.7.2",
    "url-pattern": "^1.0.3",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/chai": "4.2.5",
    "@types/js-yaml": "3.12.1",
    "@types/keycloak-connect": "^4.5.1",
    "@types/mocha": "5.2.7",
    "@types/request-promise": "4.1.45",
    "@types/swagger-ui-express": "4.1.0",
    "mocha": "10.2.0",
    "nodemon": "2.0.1",
    "npm-check": "^5.9.0",
    "oas-generator": "^2.1.5",
    "oatts": "^1.5.0",
    "request": "2.88.0",
    "ts-node": "8.5.2",
    "ts-node-dev": "^1.0.0-pre.43",
    "tslint": "5.20.1",
    "typeorm-model-generator": "^0.3.5"
  }
}
